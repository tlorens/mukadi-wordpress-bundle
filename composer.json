{
    "name": "tlorens/wordpress-bundle",
    "description": "Forked, Do not use. Another behind the times bundle.  Integrate wordpress and symfony in the same application",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier M. Mukadi",
            "email": "mbo2olivier@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "composer/installers": "1.*",
        "phpoption/phpoption": "^1.7",
        "illuminate/support": "6.7.*",
        "vlucas/phpdotenv": "5.*",
        "symfony/framework-bundle": "^5.2",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/orm": "^2.7",
        "hautelook/phpass": "^1.1",
        "mukadi/doctrine-crud": "^1.0",
        "johnpbloch/wordpress": "^5.7",
        "symfony/dotenv": "^5.2",
        "symfony/security-core": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Mukadi\\WordpressBundle\\": "src/"
        }
    },
    "suggest": {
        "doctrine/doctrine-migrations-bundle": "For a best management of your database schema"
    }
}
